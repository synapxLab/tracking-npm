  "repository": {
    "type": "git",
    "url": "git+https://github.com/synapxLab/tracking-npm.git"
  },
  "bugs": {
    "url": "https://github.com/synapxLab/tracking-npm/issues"
  },
  "homepage": "https://synapx.fr/sdk/Tracking_NPM/",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.9.0",
